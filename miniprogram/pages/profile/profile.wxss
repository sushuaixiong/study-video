/* pages/profile/profile.wxss */
page {
  background-color: #f1f1f1;
}

.container {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.back-title {
  background-image: url(data:image/jpeg;base64,);
 height: 480rpx;
 background-size: cover;
 display: flex;
 align-items: center;
 justify-content: center;
 flex-direction: column;
 color: #fff;
 font-weight: 300;
 text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.nickname {
  font-size: 36rpx;
  margin-top: 20rpx;
  font-weight: 400;
}

.profile-avatarUrl {
  width: 200rpx;
  height: 200rpx;
  display: block;
  overflow: hidden;
  border: 6rpx solid #fff;
  border-radius: 50%;
}

.nav {
  margin: 50rpx 30rpx;
  border-radius: 20rpx;
  overflow: hidden;
  box-sizing: border-box;
  display: block;
}

.nav-item {
  padding-right: 90rpx;
  padding: 0 30rpx;
  position: relative;
  display: flex;
  min-height: 100rpx;
  justify-content: space-between;
  background-color: #fff;
  align-items: center;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}

.img {
  display: inline-block;
  margin-right: 10rpx;
  width: 1.6em;
  height: 1.6em;
  vertical-align: middle;
  max-width: 100%;
}

.content {
  font-size: 30rpx;
  line-height: 1.6em;
  flex: 1;
}

.text {
  color: #808080;
}

.content .iconfont {
  color: #d43c33;
  font-weight: 600;
  margin-right: 30rpx;
}

.content .icon-xiangyou {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 30rpx;
  bottom: 0;
  color: #808080;
  font-size: 28rpx;
}